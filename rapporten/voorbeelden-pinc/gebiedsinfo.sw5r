&lt;head&gt;
&lt;layout&gt;
# 
SetNoPrinting()
SetPDFMargins(0.95, 1.5, 1, 1)
#
&lt;/layout&gt;
&lt;meta charset="utf-8" /&gt;
&lt;style&gt;
    .swingreport a {														
	color:##000;
	text-decoration:underline !important;
} /* Opmaak van hyperlinks */
&lt;/style&gt;    
&lt;title&gt;Informatie over het geselecteerde gebied&lt;/title&gt;
&lt;meta name="format-detection" content="telephone=no"/&gt;
&lt;link type="text/css" rel="stylesheet" href="style/ipo_dynamic.css" /&gt;
&lt;link type="text/css" rel="stylesheet" href="style/report_table.css" /&gt;

&lt;swingcode&gt;
&lt;inputs&gt;
#
input_geo=input
{
    Type: Geo;
    Caption: "Kies gebieden";
    Instruction: "Kies &amp;eacute;&amp;eacute;n of meerdere gebieden.";
    Level: [
    Geo.statsec,
    Geo.ggw7,
    Geo.deelgemeente,
    Geo.gemeente, 
    Geo.arrondiss,
    Geo.logo,
    Geo.politiezone,
    Geo.uitrustingsniveau,
    Geo.elz,
    Geo.fo_gem,
    Geo.gemeente2018,
    Geo.arrondiss2018,
    Geo.provincie,
    Geo.gewest
    ];
    Min: 1;
    Max: 1;
}

#

&lt;/inputs&gt;
&lt;extra&gt; 
#
/* maakt het gemakkelijker om het gekozen gebiedsniveau aan te spreken */
geolevel = level(item(input_geo, 0))
 
test_brussel = Presentation {
        Selection {
                Ind.v9900_oppervl;
                MostRecentPeriod: 1;
                input_geo;
                compare: gewest._99999;
        };
        View: Table;
}



/* maakt het gemakkelijker om het gekozen gebiedsniveau aan te spreken */
gekozen_gebiedsniveau = level(item(input_geo, 0))

/* om gemakkelijk te kunnen linken naar andere rapporten */
gebiedscode = subcode(input_geo)
gebiedsniveau = level(item(input_geo,0))
niveau_gebied = code(input_geo)

meta_gebied = code(gekozen_gebiedsniveau)

if Equal(code(gekozen_gebiedsniveau),"gemeente2018") then
meta_gebied = "gemeente"
else if Equal(code(gekozen_gebiedsniveau),"arrondiss2018") then meta_gebied = "arr"
else if Equal(code(gekozen_gebiedsniveau),"arrondiss") then meta_gebied = "arr"
else if Equal(code(gekozen_gebiedsniveau),"fo_gem") then meta_gebied = "fog"
else if Equal(code(gekozen_gebiedsniveau),"politiezone") then meta_gebied = "pz"
end if


if Equal(code(gekozen_gebiedsniveau),"statsec")
    OR  Equal(code(gekozen_gebiedsniveau),"ggw7")
    OR  Equal(code(gekozen_gebiedsniveau),"deelgemeente")
    then
        bovenliggende_gemeente=(Parent(input_geo, gemeente))
        subgemeentelijk=true
else
        subgemeentelijk=false
end if

    
if (Equal(code(gekozen_gebiedsniveau),"gemeente")
    OR  Equal(code(gekozen_gebiedsniveau),"arrondiss")
    OR  Equal(code(gekozen_gebiedsniveau),"logo") 
    OR  Equal(code(gekozen_gebiedsniveau),"politiezone") 
    OR  Equal(code(gekozen_gebiedsniveau),"gemeente2018") 
    OR  Equal(code(gekozen_gebiedsniveau),"arrondiss2018"))
    then
    subprovinciaal=true
    bovenliggende_provincie=Parent(input_geo, provincie)
else
    subprovinciaal=false
end if

if subgemeentelijk=true then
    bovenliggend_gebied = bovenliggende_gemeente
else if subprovinciaal=true then
    bovenliggend_gebied = bovenliggende_provincie
end if


if (Equal(code(gekozen_gebiedsniveau),"arrondiss")
    OR  Equal(code(gekozen_gebiedsniveau),"logo") 
    OR  Equal(code(gekozen_gebiedsniveau),"politiezone") 
    OR  Equal(code(gekozen_gebiedsniveau),"uitrustingsniveau") 
    OR  Equal(code(gekozen_gebiedsniveau),"elz")
    OR  Equal(code(gekozen_gebiedsniveau),"fo_gem")
    OR  Equal(code(gekozen_gebiedsniveau),"arrondiss2018")
    OR  Equal(code(gekozen_gebiedsniveau),"elz"))
    then
    bovengemeentelijk=true
    onderliggende_gemeenten=Descendants(input_geo, gemeente)
else
    bovengemeentelijk=false
end if


if Equal(code(gekozen_gebiedsniveau),"gewest")
    then
    onderliggende_provincies=Descendants(input_geo, provincie)
end if    
#
&lt;/extra&gt;
&lt;selections&gt;
#    

if subprovinciaal=true or subgemeentelijk=true then
introkaart_sub = Presentation {
	Selection {
		[Ind.v1101_t];
		MostRecentPeriod: 1;
		bovenliggend_gebied;
		SplitUp: geolevel;
	};
	View: Map;
	ViewDimensions: [dim_geo, dim_variable];
	LayoutClass: "introkaart";
	FavoriteGeoItem: item(input_geo,0);
}

else

introkaart_sub = Presentation {
	Selection {
		[Ind.v1101_t];
		MostRecentPeriod: 1;
		gebiedsniveau;
	};
	View: Map;
	ViewDimensions: [dim_geo, dim_variable];
	LayoutClass: "introkaart";
	FavoriteGeoItem: item(input_geo,0);
}

end if


if Equal(gekozen_gebiedsniveau, "gemeente") then

introkaart2 = Presentation {
	Selection {
		[Ind.v1101_t];
		MostRecentPeriod: 1;
		input_geo;
		SplitUp: statsec;
	};
	View: Map;
	ViewDimensions: [dim_variable, dim_period];
	LayoutClass: "introkaart";
}
    
else

introkaart2 = Presentation {
	Selection {
		[Ind.v1101_t];
		MostRecentPeriod: 1;
		input_geo;
		SplitUp: gemeente;
	};
	View: Map;
	ViewDimensions: [dim_variable, dim_period];
	LayoutClass: "introkaart";
}

end if

#   
&lt;/selections&gt;

&lt;/swingcode&gt;
